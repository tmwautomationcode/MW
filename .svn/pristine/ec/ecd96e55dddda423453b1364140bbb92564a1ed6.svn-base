	public void clickOnSiginInLink()
	{
		waitTime(2);
		if (driver.findElement(By.xpath("//section[contains(@class,'links')]//a[contains(text(),'Sign In')]")).isDisplayed()) 
		{
			clickOn(lnk_Tuxedo_SiginLink);
		}
	}


	public void fillSignInDetailsandSignIn(String EmailAddress, String Password)
	{
		waitTime(2);
		if (driver.findElement(By.xpath("//form[contains(@class,'tux-sign-in-form')]//input[contains(@id,'logonID')]")).isDisplayed()) 
		{
			typeIn(txt_Tuxedo_Sigin_EmailAddress, EmailAddress);
			typeIn(txt_Tuxedo_Sigin_Password, Password);
			clickOn(btn_Tuxedo_Sigin_SignIn);
		}
	}


	public void clickEventslinkinUserGreetingLink()
	{
		waitTime(2);
		if (driver.findElement(By.xpath("//section[contains(@class,'links')]//li//a[@id='txo-header-item-name']")).isDisplayed()) 
		{
			Actions action = new Actions(driver);
			action.moveToElement(driver.findElement(By.xpath("//section[contains(@class,'links')]//li//a[@id='txo-header-item-name']"))).build().perform();
			clickOn(lnk_Tuxedo_Signed_UserGreeting_Events);
		}
		else {
			testStepFailed("User Greeting Link is not displayed");
		}
	}
	
	
	
