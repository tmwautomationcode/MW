		public void checkRevealModalisDisplayedChecktheItemandClickContinue()
		{
			waitTime(1);
			if (driver.findElement(By.xpath("//div[contains(@class,'checkout-modal')]")).isDisplayed()) 
			{
				if (driver.findElement(By.xpath("//div[contains(@class,'checkout-modal')]//div[contains(@class,'checkout-list-item i-clearfix ng-scope active')]")).isDisplayed()) {
					clickOn(btn_Tuxedo_RevealModal_Continue);
				}
				else 
				{
					clickOn(chk_Tuxedo_RevealModal_ItemPseudoCheckbox);
					clickOn(btn_Tuxedo_RevealModal_Continue);
				}
				
				
			}
			else 
			{
				testStepPassed("Reveal Modal is not displayed");
			}
		}
		
	
	
