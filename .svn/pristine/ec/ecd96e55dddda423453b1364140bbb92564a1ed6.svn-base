		public void FillFitQuizPageDetailsandContinue(String Age, String Height, String Weight, String JeanWaist, String JeanLength, String ShoeSize)
		{
			waitTime(1);
			if (driver.findElement(By.xpath("//form[contains(@id,'take-fit-quiz')]//select[contains(@id,'FitProfileAgeSizeList')]")).isDisplayed()) 
			{
				selectFromDropdown(drp_Tuxedo_FitQuizPage_Age, Age);
				selectFromDropdown(drp_Tuxedo_FitQuizPage_Height, Height);
				selectFromDropdown(drp_Tuxedo_FitQuizPage_Weight, Weight);
				selectFromDropdown(drp_Tuxedo_FitQuizPage_JeanWaist, JeanWaist);
				selectFromDropdown(drp_Tuxedo_FitQuizPage_JeanLength, JeanLength);
				selectFromDropdown(drp_Tuxedo_FitQuizPage_ShoeSize, ShoeSize);
				clickOn(btn_Tuxedo_FitQuizPage_Continue);
			}
			else 
			{
				testStepFailed("Age Dropdown is not displayed in Fit Quiz page");
			}
		}
	
	
		
